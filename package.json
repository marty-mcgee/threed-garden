  "scripts": {
    "___________________0": "____ use these: custom scripts (add/edit your own) _____",
    "mm": "next -p 7777",
    "mm:install:full": "npm install",
    "mm:install:yarn": "yarn install",
    "mm:install:pnpm": "pnpm i",
    "mm:install:noop": "npm install --no-optional --omit_optional",
    "mm:install:wp": "pnpm wordpress-plugin",
    "___________________1": "____ use these: to delete build files __________________",
    "scrub:pnpm": "rd /S /Q .\\node_modules & del /F .\\pnpm-lock.yaml ;",
    "scrub:next": "rd /S /Q .\\.next & del /F .\\next-env.d.ts ;",
    "___________________2": "____ use these: active commons _________________________",
    "old:dev": "next",
    "next": "next -p 7777 --turbo",
    "next:dev": "next dev -p 7777 --turbo",
    "old:build": "next build",
    "next:build": "next build",
    "next:start": "next start -p 7777 --turbo",
    "next:lint": "next lint",
    "deploy": "next export",
    "next:export": "next export",
    "next:test": "tsc",
    "___________________3": "____ cleanup on aisle nine _____________________________",
    "prettier": "prettier --write **/*.{ts,tsx,js,jsx}",
    "prettier:demo": "prettier --write ./public/demo/*.{ts,tsx,js,jsx}",
    "old:lint": "eslint src",
    "lint:demo": "eslint ./public/demo/*.{ts,tsx,js,jsx}",
    "lint:fix": "pnpm lint -- --fix",
    "old:lint:next": "next lint",
    "___________________4": "____ testing, testing __________________________________",
    "test": "tsc",
    "test:jest": "jest",
    "test:tsc": "tsc",
    "test:lint": "pnpm lint && pnpm test:run",
    "___________________5": "____ scaffold-eth ______________________________________",
    "start": "next dev -p 7777",
    "dev": "next dev -p 7777",
    "build-with-ant": "yarn build:anttheme && next build",
    "build": "next build",
    "serve": "next start",
    "lint:next": "next lint",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./src",
    "build:anttheme": "yarn lessc -x -js ./src/styles/themes/ant-light-theme.less ./public/styles/ant-light-theme.css && yarn lessc -x -js ./src/styles/themes/ant-dark-theme.less ./public/styles/ant-dark-theme.css",
    "contracts:hardhat": "yarn shx rm -rf './src/generated/contracts-types' && yarn shx cp -r '../solidity-ts/generated/contract-types' './src/generated'",
    "contracts:external": "yarn shx rm -rf './src/generated/external-contracts' && yarn shx cp -r './scripts/eth-sdk-config.ts.bak' './src/generated/eth-sdk.config.ts' && yarn eth-sdk -p ./src/generated",
    "format": "yarn prettier -w ./src ",
    "format:check": "yarn prettier -c ./src ",
    "dev:link:components": "yalc link eth-components",
    "dev:link:hooks": "yalc link eth-hooks",
    "dev:unlink": "yalc remove eth-hooks && yalc remove eth-components",
    "___________________X": "____ ___________________________________________________"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
